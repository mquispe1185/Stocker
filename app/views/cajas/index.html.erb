<%= render 'negocios/menu_negocio_usuario',negocio: @negocio %>

<h1>Cajas</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cajas.each do |c| %>
      <tr>
        <td><%= "Caja #{c.nombre}: Total $#{c.total}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>


